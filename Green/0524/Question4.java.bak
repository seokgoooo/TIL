import java.util.Scanner;
public class Question4 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int input = sc.nextInt();
		int count = 0;
		
		for (int j = 2; j <= input; j++) {
			if (input % j == 0) {
				count ++;
				}
						
			if (count == 1) {
				System.out.print(j + " ");
				break;
				}
		}
	}
}