public class Box {
	int width;
	int length;
	int height;
	
	public Box(int w, int l, int h) {
		width = w;
		length = l;
		height = h;
	}
	
	public static void main(String[] args) {
		Box b = new Box(0, 0, 0);
		
		System.out.println(b.width);
		System.out.println(b.length);
		System.out.println(b.height);
	}
}